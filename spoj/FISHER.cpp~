/* bhupkas */

using namespace std;

#include "bits/stdc++.h"

const int N = 55;
const int T = 1005;
const int INF = (1<<30);

int dp[N][T];

int time[N][N];
int cost[N][N];

int n,t;

int main()
{
	while(1)
	{
		cin >> n >> t;
		
		if(n + t == 0)	break;
		
		for(int i = 0 ; i < n ; ++i)	for(int j = 0 ; j < t ; ++j)	dp[i][j] = INF;
		for(int i = 0 ; i < n ; ++i)	for(int j = 0 ; j < n ; ++j)	cin >> time[i][j];
		for(int i = 0 ; i < n ; ++i)	for(int j = 0 ; j < n ; ++j)	cin >> cost[i][j];

		for(int i = 0 ; i <= t ; ++i)
		{
			for(int j = 0 ; j < n ; ++j)
			{
				for(int k = 0 ; k < n ; ++k)
				{	
					if(
				}
			}
		}
	}
	return 0;
}
